<div class="wrapper">
    <!-- Sidebar  -->
    {{> navbar/nav-block}}

    <!-- Page Content  -->
    <div class="content" id="home">




        <div class="row">
            <div class="col-10">
                <div class="jumbotron" id="welcomeBack">
                    <div class="container">

                        <div class="row">
                            <div class="col-9">
                                <h1 style="font-weight: 600;" class="display-4 text-white"> Hello,
                                    <span class="upperCase">{{userFirstName}}</span>
                                    <span class="upperCase">{{userLastName}}</span>!
                                </h1>
                                <p class="lead text-dark">Welcome back to your Impact X dashboard.
                                </p>
                            </div>


                            <div class="col align-self-end">
                                <p id="todaysDate" style="font-weight: 400;" class="text-light"></p>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>




        <div class="line"></div>


        <div class="row">
            <div class="col">
                <div class="row">

                    <div class="col">
                        <div class="card">

                            <div class="card-body">
                                <h5 class="card-title">Recently Contacted Customers</h5>

                            </div>
                            <ul class="list-group list-group-flush" style="border: none;">
                                <li class="list-group-item">

                                    <div class="media">

                                        <a href="" class="customerBtn activate" data-toggle="modal"
                                            data-target="#exampleModalCenter">
                                            <div class="avatar" style="background-color:rgb(220, 42, 42);">WM</div>
                                        </a>

                                        <div class="media-body">

                                            <h6 class="mt-3 ml-3" id="customerName">
                                                Waylon Morris</h6>

                                        </div>
                                    </div>

                                </li>
                                <li class="list-group-item">

                                    <div class="media">

                                        <a href="" class="customerBtn activate" data-toggle="modal"
                                            data-target="#exampleModalCenter">
                                            <div class="avatar" style="background-color:rgb(220, 42, 42);">BG</div>
                                        </a>

                                        <div class="media-body">

                                            <h6 class="mt-3 ml-3" id="customerName">
                                                Beatriz Griffins</h6>

                                        </div>
                                    </div>

                                </li>
                                <li class="list-group-item">

                                    <div class="media">

                                        <a href="" class="customerBtn activate" data-toggle="modal"
                                            data-target="#exampleModalCenter">
                                            <div class="avatar" style="background-color:rgb(220, 42, 42);">AS</div>
                                        </a>

                                        <div class="media-body">

                                            <h6 class="mt-3 ml-3" id="customerName">
                                                Angela Singh</h6>

                                        </div>
                                    </div>

                                </li>
                                <li class="list-group-item">

                                    <div class="media">

                                        <a href="" class="customerBtn activate" data-toggle="modal"
                                            data-target="#exampleModalCenter">
                                            <div class="avatar" style="background-color:rgb(220, 42, 42);">BM</div>
                                        </a>

                                        <div class="media-body">

                                            <h6 class="mt-3 ml-3" id="customerName">
                                                Doyle Mendozas</h6>

                                        </div>
                                    </div>

                                </li>

                            </ul>
                            <div class="card-body">
                                <a style="float: right; color:blue" href="/contacts" class="card-link">View Contacts</a>

                            </div>
                        </div>
                    </div>
                    <div class="container col">
                        <div class="card">
                            <div class="row justify-content-center">

                                <div class="col text-center jus">

                                    <button class="btn btn-outline-info" id="previous" onclick="previous()"><i
                                            class="fas fa-arrow-left"></i></button>
                                    <span>
                                        <h4 class="mx-3" id="monthAndYear"></h4>
                                    </span>

                                    <button class="btn btn-outline-info" id="next" onclick="next()"><i
                                            class="fas fa-arrow-right"></i></button>

                                </div>

                            </div>

                            <table class="table table-bordered table-responsive-sm" id="calendar">
                                <thead>
                                    <tr>
                                        <th>Sun</th>
                                        <th>Mon</th>
                                        <th>Tue</th>
                                        <th>Wed</th>
                                        <th>Thu</th>
                                        <th>Fri</th>
                                        <th>Sat</th>
                                    </tr>
                                </thead>

                                <tbody id="calendar-body">

                                </tbody>
                            </table>

                            <br />
                            <form class="form-inline" hidden>
                                <label class="lead mr-2 ml-2" for="month">Jump To: </label>
                                <select class="form-control col-sm-4" name="month" id="month" onchange="jump()">
                                    <option value=0>Jan</option>
                                    <option value=1>Feb</option>
                                    <option value=2>Mar</option>
                                    <option value=3>Apr</option>
                                    <option value=4>May</option>
                                    <option value=5>Jun</option>
                                    <option value=6>Jul</option>
                                    <option value=7>Aug</option>
                                    <option value=8>Sep</option>
                                    <option value=9>Oct</option>
                                    <option value=10>Nov</option>
                                    <option value=11>Dec</option>
                                </select>


                                <label for="year"></label><select class="form-control col-sm-4" name="year" id="year"
                                    onchange="jump()">
                                    <option value=2020>2020</option>
                                    <option value=2021>2021</option>
                                    <option value=2022>2022</option>
                                    <option value=2023>2023</option>
                                    <option value=2024>2024</option>
                                    <option value=2025>2025</option>
                                    <option value=2026>2026</option>
                                    <option value=2027>2027</option>
                                    <option value=2028>2028</option>
                                    <option value=2029>2029</option>
                                    <option value=2030>2030</option>
                                </select></form>
                        </div>
                    </div>

                </div>
            </div>
            <div class="col-4">
                <div class="container">
                    <div class="row">
                        <div class="col-sm">
                            <div class="card text-white bg-info mb-3 analytics" id="grad1">

                                <div class="card-body">
                                    <div class="media">
                                        <div class="media-body">
                                            <h3 id="totalTickts">0</h3>
                                            <h6 class="mt-0 mb-1">Total Tickets</h6>

                                        </div>
                                        <h2 class="align-self-center">
                                            <i class="fas fa-inbox"></i>
                                        </h2>
                                    </div>


                                </div>
                            </div>
                        </div>
                        <div class="w-100"></div>
                        <div class="col">
                            <div class="card text-white bg-info mb-3 analytics" id="grad2">

                                <div class="card-body">
                                    <div class="media">
                                        <div class="media-body">
                                            <h3 id="customerCount">2345</h3>
                                            <h6 class="mt-0 mb-1">Total # of Customers</h6>

                                        </div>
                                        <h2 class="align-self-center ml-2">
                                            <i class="fas fa-address-card"></i>
                                        </h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="w-100"></div>

                        <div class="col">
                            <div class="card text-white bg-info mb-3 analytics" id="grad3">

                                <div class="card-body">
                                    <div class="media">
                                        <div class="media-body">
                                            <h3 id="openTicketCount">2345</h3>
                                            <h6 class="mt-0 mb-1">Total Open Tickets</h6>

                                        </div>
                                        <h2 class="align-self-center">
                                            <i class="fas fa-envelope-open-text"></i>
                                        </h2>
                                    </div>


                                </div>
                            </div>
                        </div>
                        <div class="w-100"></div>
                        <div class="col">
                            <div class="card text-white bg-info mb-3 analytics" id="grad4">

                                <div class="card-body">
                                    <div class="media">
                                        <div class="media-body">
                                            <h3 id="closedTicketCount">2345</h3>
                                            <h6 class="mt-0 mb-1">Total Closed Tickets</h6>

                                        </div>
                                        <h2 class="align-self-center">
                                            <i class="fas fa-envelope"></i>
                                        </h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
